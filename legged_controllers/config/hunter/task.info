}

model_settings
{
  positionErrorGain             20    
  phaseTransitionStanceTime     0.1
  gravityCompensation           false
  visualizeIK                   false

  verboseCppAd                  false
  recompileLibrariesCppAd       false
  modelFolderCppAd              /tmp/bridge/legged_control/hunter
}


swing_trajectory_config
{
  liftOffVelocity               0.01    //0.035
  touchDownVelocity             0.0     //0.0
  swingHeight                   0.05
  swingTimeScale                0.03

  feet_bias_x1                  0.144     //0.034  0.224
  feet_bias_x2                  0.054    //-0.056  0.134
