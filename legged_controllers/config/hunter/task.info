; standard state weight matrix
Q
{
  scaling 1e+0

  ;; Normalized Centroidal Momentum: [linear, angular] ;;
  (0,0)   13.0        ; vcom_x //线速度
  (1,1)   130.0        ; vcom_y
  (2,2)   13.0        ; vcom_z
  (3,3)   26.0        ; L_x / robotMass 角动量/质量
  (4,4)   13.0        ; L_y / robotMass
  (5,5)   13.0        ; L_z / robotMass

  ;; Base Pose: [position, orientation] ;;
  (6,6)   500.0    ; p_base_x
  (7,7)   500.0    ; p_base_y
  (8,8)   500.0    ; p_base_z
  (9,9)   100.0    ; theta_base_z
  (10,10) 500.0    ; theta_base_y
  (11,11) 800.0    ; theta_base_x
 
  ;; Leg Joint Positions: [L, R] ;;
  (12,12)  100.0     ; leg_l1_joint
  (13,13)  5.0     ; leg_l2_joint
  (14,14)  10.0     ; leg_l3_joint
  (15,15)  10.0     ; leg_l4_joint
  (16,16)  10.0     ; leg_l5_joint
  (17,17)  100.0     ; leg_r1_joint
  (18,18)  5.0     ; leg_r2_joint
  (19,19)  10.0     ; leg_r3_joint
  (20,20)  10.0     ; leg_r4_joint
  (21,21)  10.0     ; leg_r5_joint
}

; control weight matrix
R
{
  scaling 1e-3

  ;; Front Feet Contact Forces: [L, R] ;;
  (0,0)   5.0    ; left_force
  (1,1)   5.0    ; left_force
  (2,2)   5.0    ; left_force
  (3,3)   5.0    ; right_force
  (4,4)   5.0    ; right_force
  (5,5)   5.0    ; right_force
  ;; Back Feet Contact Forces: [L, R]
  (6,6)   5.0    ; left_force
  (7,7)   5.0    ; left_force
  (8,8)   5.0    ; left_force
  (9,9)   5.0    ; right_force
  (10,10) 5.0    ; right_force
  (11,11) 5.0    ; right_force

  ;; left foot velocity relative to base: [linear(3), angular(3)] (uses the Jacobian at nominal configuration) ;;
